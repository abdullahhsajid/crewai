{
    "version":2,
    "builds": [
        {
            "src":"main.py",
            "use":"@vercel/python"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"main.py"
        }
    ],
    "crons":[
        {
            "path":"/api/cron.py",
            "schedule":"39 18 * * *"
        }
    ]
}
